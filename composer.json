{
  "name": "izniburak/nur-core",
  "type": "library",
  "description": "Nur Framework Core Files. https://github.com/izniburak/nur",
  "keywords": [
    "php",
    "framework",
    "nur",
    "simple",
    "basic"
  ],
  "homepage": "https://github.com/izniburak/nur-core",
  "license": "MIT",
  "authors": [
    {
      "name": "İzni Burak Demirtaş",
      "email": "info@burakdemirtas.org",
      "homepage": "http://burakdemirtas.org",
      "role": "Creator"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "izniburak/nur-migration": "^1",
    "izniburak/pdox": "^1",
    "izniburak/router": "^2",
    "illuminate/database": "^8.0",
    "illuminate/pagination": "^8.0",
    "illuminate/view": "^8.0",
    "illuminate/events": "^8.0",
    "illuminate/translation": "^8.0",
    "illuminate/cache": "^8.0",
    "symfony/console": "^5.1",
    "symfony/http-foundation": "^5.1",
    "symfony/var-dumper": "^5.1",
    "symfony/cache": "^5.1",
    "symfony/process": "^5.1",
    "ramsey/uuid": "^4.0",
    "vlucas/phpdotenv": "^5.2",
    "firebase/php-jwt": "^5.0",
    "phpmailer/phpmailer": "~6.1"
  },
  "require-dev": {
    "doctrine/dbal": "^2.6|^3.0",
    "filp/whoops": "^2.8"
  },
  "autoload": {
    "files": [
      "src/helper.php"
    ],
    "psr-4": {
      "Nur\\": "src/"
    }
  },
  "support": {
    "email": "info@burakdemirtas.org"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
